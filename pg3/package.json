{
  "name": "pg-step3-enrichment",
  "version": "1.0.0",
  "description": "PG3 enrichment pipeline",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp -r src/LANDING dist/src/",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "npm run typecheck",
    "test:unit": "OPENAI_API_KEY=test-key vitest run tests/unit --reporter=dot",
    "test:smoke": "OPENAI_API_KEY=test-key REDIS_URL=redis://127.0.0.1:6379/15 vitest run tests/integration --reporter=dot",
    "test": "npm run test:unit && npm run test:smoke",
    "dev:worker": "ts-node src/index.ts worker",
    "dev:scheduler": "ts-node src/index.ts scheduler",
    "start": "node dist/src/index.js worker",
    "start:worker": "node dist/src/enricher/worker.js",
    "start:scheduler": "node dist/src/index.js scheduler",
    "start:server": "node dist/src/index.js server"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/node": "^25.0.9",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.67.3",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fast-csv": "^5.0.5",
    "fast-levenshtein": "^3.0.0",
    "ghost-cursor": "^1.4.2",
    "got-scraping": "^4.1.3",
    "ioredis": "^5.9.2",
    "leven": "^4.1.0",
    "libphonenumber-js": "^1.12.36",
    "md5": "^2.3.0",
    "neo4j-driver": "^6.0.1",
    "openai": "^6.16.0",
    "p-limit": "^7.2.0",
    "poolifier": "^5.1.7",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.36.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-real-browser": "^1.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "vitest": "^3.2.4"
  }
}